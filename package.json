{
  "name": "react-express-bacchus",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node index.js && npm run build",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build"
  },
  "engines": {
    "node": "12.8.1",
    "npm": "6.x"
  },
  "loader": "css-loader",
  "options": {
    "minimize": false
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@auth0/auth0-spa-js": "^1.5.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/react-fontawesome": "^0.1.7",
    "axios": "^0.19.0",
    "bootstrap": "^4.4.0",
    "cheerio": "^1.0.0-rc.3",
    "cookie-parser": "^1.4.4",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "highlight.js": "^9.16.2",
    "http-errors": "^1.7.3",
    "mongoose": "^5.7.13",
    "morgan": "^1.9.1",
    "request": "^2.88.0",
    "yargs": "^15.0.2"
  },
  "devDependencies": {
    "concurrently": "^5.0.0",
    "if-env": "^1.0.4",
    "nodemon": "^1.19.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BullHamund/Bacchus.git"
  },
  "bugs": {
    "url": "https://github.com/BullHamund/Bacchus/issues"
  },
  "homepage": "https://github.com/BullHamund/Bacchus#readme",
  "description": ""
}
