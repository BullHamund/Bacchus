    "acorn": "^8.0.1",
    "axios": "^0.19.0",
    "bootstrap": "^4.4.0",
    "cheerio": "^1.0.0-rc.3",
    "cookie-parser": "^1.4.4",
    "debug": "^4.1.1",
    "dot-prop": "^5.3.0",
    "dotenv": "^8.2.0",
    "elliptic": "^6.5.3",
    "express": "^4.17.1",
    "highlight.js": "^10.4.1",
    "http-errors": "^1.7.3",
    "if-env": "^1.0.4",
    "kind-of": "^6.0.3",
    "lodash": "^4.17.20",
    "minimist": "^1.2.5",
    "mongoose": "^5.7.13",
    "morgan": "^1.9.1",
    "request": "^2.88.0",
